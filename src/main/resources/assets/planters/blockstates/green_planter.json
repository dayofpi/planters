{
  "multipart": [
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "planters:block/green_planter",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "planters:block/green_planter"
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "planters:block/green_planter",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "planters:block/green_planter",
        "y": 270
      }
    },
    {
      "when": {
        "hanging": "true",
        "facing": "east"
      },
      "apply": {
        "model": "planters:block/planter_chains",
        "y": 90
      }
    },
    {
      "when": {
        "hanging": "true",
        "facing": "north"
      },
      "apply": {
        "model": "planters:block/planter_chains"
      }
    },
    {
      "when": {
        "hanging": "true",
        "facing": "south"
      },
      "apply": {
        "model": "planters:block/planter_chains",
        "y": 180
      }
    },
    {
      "when": {
        "hanging": "true",
        "facing": "west"
      },
      "apply": {
        "model": "planters:block/planter_chains",
        "y": 270
      }
    }
  ]
}
